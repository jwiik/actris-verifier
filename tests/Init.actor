actor Repeat int in ==> int out :
  action in:[i] ==> out:[i] end
end

actor Delay(int k) int in ==> int out :
  initialize ==> out:[k] end
  action in:[i] ==> out:[i] end
end

network Net int in ==> int out :
  
  action in:1 ==> out:1 end
  
  invariant tokens(b,1)
  
  entities
    del = Delay(0);
    rep = Repeat();
  end
  
  structure
    a: in --> del.in;
    b: del.out --> rep.in;
    c: rep.out --> out;
  end

end