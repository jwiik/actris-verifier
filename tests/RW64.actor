actor RW64 int X ==> int Y:
  
  int count := 0;
  
  read: 
  action X:[i] ==> Y:[i]
  guard
    count < 5
  do 
    count := count+1;
  end
  
  done:
  action ==>
  guard
    count = 5
  do
    count := 0
  end
  

end

network Net int X ==> int Y:

  contract X:5 ==> Y:5 end
  
  entities
    rw64 = RW64();
  end
  
  structure
    a: X --> rw64.X;
    b: rw64.Y --> Y;
  end
  
end