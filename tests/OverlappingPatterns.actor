
actor Overlapping bool G, int X1, int X2 ==> int Y:
  A: action G:[g], X1:[i] ==> Y:[0] end
  B: action G:[g], X1:[i,k], X2:[j] ==> Y:[0] end
  C: action G:[g], X1:[i] ==> Y:[0] end
  D: action X2:[j]  ==> Y:[0] end
  E: action  ==> Y:[0] end
end

/*
actor Overlapping bool G, int X1, int X2 ==> int Y:
  A: action G:[g], X1:[i] ==> Y:[0] end
  C: action G:[g], X1:[i] ==> Y:[0] end
end
*/

network Net bool g, int in1, int in2 ==> int out :
    
  contract g:1, in1:1 ==> out:1 end
  
  
  entities
    a = Overlapping();
  end
  
  structure
    a: in1 --> a.X1;
    b: in2 --> a.X2;
    c: g --> a.G;
    d: a.Y --> out;
  end

end