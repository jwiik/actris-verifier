actor TimeDep int in ==> int out :
  A: action in:[i] ==> out:[i] end
  B: action in:[i,j] ==> out:[i] end
  priority A > B end
end

network Net int in ==> int out :
  
  action in:[x] ==> out:[y] end
  
  chinvariant @(a) = @(b)
  chinvariant tot(b) = rd(a)

  entities
    tp = TimeDep();
  end
  
  structure
    a: in --> tp.in;
    b: tp.out --> out;
  end

end